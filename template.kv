<MenuScreen>:
    StackLayout:
        Button:
            text: 'Woordjes Leren'
            size_hint: (.2, .1)
            on_press: root.manager.current = 'settings'

        Button:
            text: 'Agenda'
            size_hint: (.2, .1)
            on_press: root.manager.current = 'agenda'

        Button:
            text: 'Cijfers Berekenen'
            size_hint: (.2, .1)
            on_press: root.manager.current = 'calculator'

        Button:
            text: "play Learn Sound"
            size_hint: (.2, .1)
            on_release: root.play_sound()

    BoxLayout:
        orientation: "vertical"
        size: root.width, root.height
        Label:
            id: name_pieces
            text: "Your Current Tasks: "
            font_size: 20

        GridLayout:
            cols:3

            CheckBox:
                on_active: root.checked(self, self.active)

            Label:
                id: list_label
                color: 1,1,0,1
                text: " "
                font_size: 15


        Label:
            id: name_label
            color: 0,1,1,1
            text: " "
            font_size: 15

        Button:
            id: name_label
            size_hint: .2, .5
            text: "Done"
            font_size: 15
            on_press: root.deletetodo()

        TextInput:
            id: name_input
            multiline: False
            size_hint: (1, .5)

        Button:
            size_hint: (1, .5)
            font_size: 32
            text: "Submit"
            on_press: root.main()

<SettingsScreen>:
    BoxLayout:
        orientation: "vertical"
        size: root.width, root.height

        Button:
            id: goback
            size_hint: (.15, .5)
            text: "Ga terug"
            on_press: root.manager.current = 'menu'


        Label:
            id: words
            color: 0,1,1,1
            text: "Typ je woordjes in de lijst met een '=' tussen de verschillende woordjes, en gebruik een Enter voor elke rij"
            font_size: 16

        TextInput:
            id: user_input
            multiline: True

        Button:
            text: 'Woordjes Opslaan'
            on_press: root.register()
        Button:
            text: 'Woordjes Overhoren'
            on_press: root.manager.current = 'overhoren'
        Button:
            text: 'Leer Woorjdes Die verkeerd gingen'
            on_press: root.manager.current = 'overhoren1'

